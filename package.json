{
  "name": "ddw-analyst-ui",
  "version": "1.0.1",
  "description": "The new and improved DDW Analyst UI interface",
  "main": "index.js",
  "scripts": {
    "dev": "grunt",
    "build": "grunt release",
    "test": "jest --coverage && python3 manage.py test --settings=ddw_analyst_ui.test_settings",
    "test:docker": "jest && docker-compose run web python3 manage.py test --settings=ddw_analyst_ui.docker_test_settings",
    "collectstatic": "python3 manage.py collectstatic --no-input",
    "docker:dev": "grunt 'docker dev'",
    "docker:build": "docker-compose up --build",
    "docker:collectstatic": "docker-compose run web python3 manage.py collectstatic --no-input",
    "emit": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx' && tsc --noEmit",
    "coverage": "NODE_ENV=test jest --coverage && codecov && docker-compose run web python3 -m coverage run manage.py test --settings=ddw_analyst_ui.docker_test_settings && docker-compose run web python3 -m coverage xml && python3 -m codecov",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devinit/ddw-analyst-ui.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devinit/ddw-analyst-ui/issues"
  },
  "homepage": "https://github.com/devinit/ddw-analyst-ui#readme",
  "devDependencies": {
