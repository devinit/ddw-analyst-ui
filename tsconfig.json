{
    "compilerOptions": {
      "rootDir": "./frontend/src",
      "outDir": "./tsc",
        "target": "esnext",
        "lib": [
          "dom",
          "dom.iterable",
          "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "strictFunctionTypes": false,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true
    },
    "exclude": [
        "node_modules/"
    ],
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx"
    ]
}
